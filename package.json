{
  "name": "delightful-ui",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^7.0.0-beta.34",
    "@storybook/addon-interactions": "^7.0.0-beta.34",
    "@storybook/addon-links": "^7.0.0-beta.34",
    "@storybook/blocks": "^7.0.0-beta.34",
    "@storybook/svelte": "^7.0.0-beta.34",
    "@storybook/sveltekit": "^7.0.0-beta.34",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^1.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.34",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.1",
    "svelte-loader": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module"
}
